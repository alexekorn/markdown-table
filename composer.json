{
    "name": "kba-team/markdown-table",
    "description": "Create a markdown table containing your data.",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/the-kbA-team/markdown-table/issues"
    },
    "keywords": [
        "markdown",
        "table",
        "markdown-table",
        "array"
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^5.5|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "kbATeam\\MarkdownTable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\kbATeam\\MarkdownTable\\": "tests/"
        }
    }
}
